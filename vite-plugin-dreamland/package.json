{
  "name": "vite-plugin-dreamland",
  "version": "1.0.1",
  "description": "A simple Vite plugin for dreamland.js",
  "main": "dist/index.js",
  "scripts": {
    "types": "tsc --declaration --emitDeclarationOnly --outDir dist/ --declarationMap",
    "type-check": "tsc --noEmit",
    "build": "babel src/ --out-dir dist/ --extensions '.ts'",
    "publish": "npm publish --access public"
  },
  "keywords": [
    "vite",
    "plugin",
    "dreamland",
    "dreamland.js",
    "dreamlandjs",
    "js",
    "javascript"
  ],
  "author": "MotorTruck1221",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  }
}
