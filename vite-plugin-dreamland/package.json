{
  "name": "vite-plugin-dreamland",
  "version": "1.2.0",
  "description": "A simple Vite plugin for dreamland.js",
  "main": "dist/index.js",
  "scripts": {
    "types": "tsc --declaration --emitDeclarationOnly --outDir dist/ --declarationMap",
    "type-check": "tsc --noEmit",
    "build": "babel src/ --out-dir dist/ --extensions '.ts'",
    "release": "npm publish --access public"
  },
  "keywords": [
    "vite",
    "plugin",
    "dreamland",
    "dreamland.js",
    "dreamlandjs",
    "js",
    "javascript"
  ],
  "author": "MotorTruck1221",
  "license": "MIT", 
  "repository": {
    "type": "git",
    "url": "git+https://github.com/motortruck1221/dreamland-stuff.git"
  },  
  "bugs": {
    "url": "https://github.com/motortruck1221/dreamland-stuff/issues"
  },
  "homepage": "https://github.com/motortruck1221/dreamland-stuff#readme",
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  }
}
