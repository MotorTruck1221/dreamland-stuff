{
    "include": ["src/**/*"],
    "compilerOptions": {
        "target": "es2016",
        "module": "esnext",
        // "rootDir": "./",
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    }
}
