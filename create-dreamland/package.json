{
  "name": "create-dreamland",
  "version": "1.0.0",
  "description": "Create a Dreamland app with all the stuffs setup (alias to create-dreamland-app)",
  "main": "dist/index.js",
  "bin": {
    "create-dreamland": "dist/index.js"
  },
  "type": "module",
  "scripts": {
    "types": "tsc --declaration --emitDeclarationOnly --outDir dist/ --declarationMap",
    "type-check": "tsc --noEmit",
    "build": "babel src/ --out-dir dist/ --extensions '.ts' --copy-files",
    "release": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/motortruck1221/dreamland-stuff.git"
  },
  "keywords": [
    "javascript",
    "dreamland",
    "dreamland-js",
    "dreamlandjs"
  ],
  "author": "MotorTruck1221",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/motortruck1221/dreamland-stuff/issues"
  },
  "homepage": "https://github.com/motortruck1221/dreamland-stuff#readme",
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.12.10",
    "typescript": "^5.4.5",
    "@babel/plugin-syntax-import-attributes": "^7.24.1"
  },
  "dependencies": {
      "create-dreamland-app": "latest"
  }
}
